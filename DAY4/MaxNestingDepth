class Solution(object):
    def maxDepth(self, s):
        """
        :type s: str
        :rtype: int
        """
        depth=0
        maxdep=0
        for ch in s:
            if ch=='(':
                depth+=1
                maxdep=max(maxdep,depth)
            elif ch==')':
                depth-=1
        return maxdep    
